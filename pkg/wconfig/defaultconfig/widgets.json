{
    "defwidget@aipy": {
        "display:order": -7,
        "icon": "./icons/aipy.png",
        "label": "aipy",
        "blockdef": {
            "meta": {
                "view": "term",
                "controller": "cmd",
                "cmd": "$AIPY_TOOLS/run_aipyapp.sh",
                "shell_cmd": "$AIPY_TOOLS/run_aipyapp.sh",
                "win_cmd": "& \"$Env:AIPY_TOOLS\\run_aipyapp.ps1\"",
                "_comment": "emain.ts 处理cmd"
            }
        }
    },
    "defwidget@prompt": {
        "display:order": -6,
        "icon": "./icons/prompt.png",
        "label": "prompt",
        "blockdef": {
            "meta": {
                "view": "term",
                "controller": "cmd",
                "cmd": "$AIPY_TOOLS/run_prompt.sh",
                "shell_cmd": "$AIPY_TOOLS/run_prompt.sh",
                "win_cmd": "& \"$Env:AIPY_TOOLS\\run_prompt.ps1\"",
                "_comment": "emain.ts 处理cmd"
            }
        }
    },
    "defwidget@files": {
