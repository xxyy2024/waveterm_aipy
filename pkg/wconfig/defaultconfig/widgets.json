{
    "defwidget@aipy": {
        "display:order": -6,
        "icon": "icons/aipy.png",
        "label": "aipy",
        "blockdef": {
            "meta": {
                "view": "term",
                "controller": "cmd",
                "cmd": "$AIPY_TOOLS/run_aipyapp.sh",
                "shell_cmd": "$AIPY_TOOLS/run_aipyapp.sh",
                "win_cmd": "& \"$Env:AIPY_TOOLS\\run_aipyapp.ps1\"",
                "_comment": "emain.ts 处理cmd"
            }
        }
    },
    "defwidget@prompt": {
        "display:order": -5,
        "icon": "icons/prompt.png",
        "label": "prompt",
        "blockdef": {
            "meta": {
                "view": "term",
                "controller": "cmd",
                "cmd": "$AIPY_TOOLS/run_prompt.sh",
                "shell_cmd": "$AIPY_TOOLS/run_prompt.sh",
                "win_cmd": "& \"$Env:AIPY_TOOLS\\run_prompt.ps1\"",
                "_comment": "emain.ts 处理cmd"
            }
        }
    },
    "defwidget@files": {
        "display:order": -4,
        "icon": "icons/files.png",
        "label": "files",
        "blockdef": {
            "meta": {
                "view": "preview",
                "file": "~/work"
            }
        }
    },
    "defwidget@web": {
        "display:order": -3,
        "icon": "icons/web.png",
        "label": "web",
        "blockdef": {
            "meta": {
                "view": "web"
            }
        }
    },
    "defwidget@sysinfo": {
        "display:order": -2,
        "icon": "icons/sysinfo.png",
        "label": "sysinfo",
        "blockdef": {
            "meta": {
                "view": "sysinfo"
            }
        }
    },
    "defwidget@terminal": {
        "display:order": -1,
        "icon": "icons/terminal.png",
        "label": "terminal",
        "blockdef": {
            "meta": {
                "view": "term",
                "controller": "shell"
            }
        }
    }
}