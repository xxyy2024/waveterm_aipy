{
    "bg@default": {
        "display:name": "Default",
        "display:order": -1,
        "bg:*": true
    },
    "bg@rainbow": {
        "display:name": "Rainbow",
        "display:order": 1,
        "bg:*": true,
        "bg": "linear-gradient( 226.4deg,  rgba(255,26,1,1) 28.9%, rgba(254,155,1,1) 33%, rgba(255,241,0,1) 48.6%, rgba(34,218,1,1) 65.3%, rgba(0,141,254,1) 80.6%, rgba(113,63,254,1) 100.1% )",
        "bg:opacity": 0.3
    },
    "bg@green": {
        "display:name": "Green",
        "bg:*": true,
        "bg": "green",
        "bg:opacity": 0.3
    },
    "bg@blue": {
        "display:name": "Blue",
        "bg:*": true,
        "bg": "blue",
        "bg:opacity": 0.3
    },
    "bg@red": {
        "display:name": "Red",
        "bg:*": true,
        "bg": "red",
        "bg:opacity": 0.3
    }
}
